<h1>User info</h1>
  <p>画像</p>
  <% if list.image.attached? %>
    <%= image_tag list.image, size: "200x200" %>
  <% else %>
    <%= image_tag 'no_image', size: "200x200" %>
  <% end %>
  <h1>New book</h1>
    <%= form_with model: @book.new do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>

    <h4>Opinion</h4>
    <%= f.text_area :body %>

    <%= f.submit 'Create Book' %>
  <% end %>
  
  <h1>Book detail</h1>

  <%= @books.each do |list| %>

  <p>タイトル</p>
  <span><%= list.title %></span>
  
<% end %>